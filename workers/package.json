{
  "name": "workers",
  "version": "1.0.0",
  "type": "module",
  "description": "AI workers for prediction market",
  "scripts": {
    "dev:generator": "tsx watch src/generator.ts",
    "dev:validator": "tsx watch src/validator.ts",
    "dev:publisher": "tsx watch src/publisher.ts",
    "dev:resolver": "tsx watch src/resolver.ts",
    "dev:scheduler": "tsx watch src/scheduler.ts",
    "dev:dispute-agent": "tsx watch src/dispute-agent.ts",
    "dev:crawler": "tsx watch src/crawler.ts",
    "dev:extractor": "tsx watch src/extractor.ts",
    "build": "tsc",
    "start:generator": "node dist/generator.js",
    "start:validator": "node dist/validator.js",
    "start:publisher": "node dist/publisher.js",
    "start:resolver": "node dist/resolver.js",
    "start:scheduler": "node dist/scheduler.js",
    "start:dispute-agent": "node dist/dispute-agent.js",
    "start:crawler": "node dist/crawler.js",
    "start:extractor": "node dist/extractor.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test:all": "tsx scripts/test-all.ts",
    "test:quick": "tsx scripts/test-all.ts all --quick",
    "test:backend": "tsx scripts/test-all.ts backend",
    "test:database": "tsx scripts/test-all.ts database",
    "test:queue": "tsx scripts/test-all.ts queue",
    "test:ai": "tsx scripts/test-all.ts ai",
    "test:workers": "tsx scripts/test-all.ts workers",
    "test:e2e": "tsx scripts/test-all.ts e2e",
    "test:integration": "tsx scripts/test-all.ts integration"
