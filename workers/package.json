  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.98.0",
    "@x402/shared-types": "workspace:*",
    "amqplib": "^0.10.9",
    "bs58": "^6.0.0",
    "dotenv": "^17.2.3",
    "node-cron": "^3.0.3",
    "openai": "^4.76.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.7",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/amqplib": "^0.10.8",
    "@types/bs58": "^5.0.0",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.49.0"
  }
