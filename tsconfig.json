{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types"],
    "lib": ["es2015"],
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "declaration": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
